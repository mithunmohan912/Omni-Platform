@objects
    userid				id      inputUsername
    password		id      inputPassword         
    submit				id      submit
	useridLabel	css	label[for=inputUsername]
	passwordLabel   css   label[for=inputPassword]
    
= Main Section =
	@on desktop
		userid:
		   above password 10 to 20 px
		   
		password:
		   above submit 10 to 20 px
		   
	@on mobile
		useridLabel:
		   above userid 5 to 10 px
		   
		passwordLabel:
		   above password 5 to 10 px

	@on tablet
		useridLabel:
		   above userid 5 to 10 px
		   
		passwordLabel:
		   above password 5 to 10 px