@objects
  make				id		quote_van:make
  model				id		quote_van:ext_model
  power				id		quote_van:power
    
= form =
	@on desktop
		make:
			above model 5 to 25px
		model:
			above power 5 to 25px
	@on mobile
		make:
			above model 20 to 35px
		model:
			above power 20 to 35px
	@on tablet
		make:
			above model 20 to 35px
		model:
			above power 20 to 35px