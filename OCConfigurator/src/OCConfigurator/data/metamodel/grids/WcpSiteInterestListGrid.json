  "dataSource": {
    "type": "SQL",
    "filename": "PMSP1200",
    "SQL": "SELECT TRANS0STAT, DEEMSTAT, DESC0LINE1, USE0CODE, DESC0SEQ FROM PMSP1200 WHERE TRANS0STAT='V' AND SYMBOL=@sym@ AND POLICY0NUM=@polno@ AND MODULE=@mod@ AND MASTER0CO=@mco@ AND LOCATION=@loc@ AND USE0LOC =@site@ AND FLAG = 'Y' and not(USE0CODE in ( select INTTYPE from TBWC77) and DEEMSTAT = 'V')",
    "pagesize": "10",
    "params": {
      "sym": [
        {
          "type": "KEY",
	  "startIndex": "5",
          "endIndex": "8"
        }
      ],
      "polno": [
        {
          "type": "KEY",
	  "startIndex": "8",
          "endIndex": "15"
        }
      ],
      "mod": [
        {
          "type": "KEY",
          "startIndex": "15",
          "endIndex": "17"
        }
      ],
      "mco": [
        {
          "type": "KEY",
          "startIndex": "3",
          "endIndex": "5"
        }
      ],
      "loc": [
        {
          "type": "KEY",
          "startIndex": "1",
          "endIndex": "3"
        }
      ],
      "site": [
        {
          "type": "KEY",
          "startIndex": "96",
          "endIndex": "101"
        }
      ]
    }
  }
