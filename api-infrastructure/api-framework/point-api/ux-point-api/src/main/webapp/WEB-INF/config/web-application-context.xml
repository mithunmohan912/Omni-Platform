		<camel:route id="exceptionHandler">
			<camel:from uri="direct:exceptionHandler" />

			<camel:filter>
				<camel:xpath>/*/Status/StatusCd ='SystemError'</camel:xpath>
				<camel:to ref="exceptionResponseXML" />
			</camel:filter>
		</camel:route>

		<camel:route id="service">
