	<util:properties id="app-config" location="classpath:app-config.properties" />
	<util:properties id="surround-config" location="classpath:surround_products.properties" />
	<util:properties id="navigation-config" location="classpath:navigation_processor.properties" />
	<util:properties id="modifier-config" location="classpath:policy_modifier.properties" />

	<!-- Imports the configurations of the different infrastructure systems of the application -->
	<import resource="mvc-config.xml" />
	<import resource="data-access-config.xml" />
	<import resource="classpath:META-INF/config/application-context.xml" />

	<!-- Service Processors -->

	<!-- XStream Delegate -->
	<bean id="xStreamServiceProcessor" class="com.csc.cscip.ux.common.rest.processor.xml.XStreamServiceProcessor" />

	<!-- Point JDBC XML Delegate -->
	<bean id="pointJDBCXMLProcessor" class="com.csc.cscip.ux.pointin.rest.processor.PointJDBCXMLProcessor">
		<property name="delegate" ref="xStreamServiceProcessor"></property>
	</bean>

	<!-- Data -->
	<bean id="dataProcessor" class="com.csc.cscip.ux.pointin.rest.processor.DataProcessor">
		<property name="delegate" ref="xStreamServiceProcessor"></property>
	</bean>

	<!-- LRARO -->
	<bean id="editableGridDataProcessor" class="com.csc.cscip.ux.pointin.rest.processor.EditableGridDataProcessor">
		<property name="delegate" ref="xStreamServiceProcessor"></property>
	</bean>

	<!-- Navigation Data -->
	<bean id="navigationDataProcessor" class="com.csc.cscip.ux.pointin.rest.processor.NavigationDataProcessor">
		<property name="delegate" ref="xStreamServiceProcessor"></property>
	</bean>

	<!-- Support Data -->
	<bean id="supportDataProcessor" class="com.csc.cscip.ux.pointin.rest.processor.SupportDataProcessor">
		<property name="delegate" ref="pointJDBCXMLProcessor"></property>
	</bean>

	<!-- Grid Data -->
	<bean id="gridDataProcessor" class="com.csc.cscip.ux.pointin.rest.processor.GridDataProcessor">
		<property name="delegate" ref="pointJDBCXMLProcessor"></property>
	</bean>

	<!-- Action -->
	<bean id="actionProcessor" class="com.csc.cscip.ux.pointin.rest.processor.ActionProcessor">
		<property name="delegate" ref="pointJDBCXMLProcessor"></property>
	</bean>

	<!-- Media -->
	<bean id="mediaURLResolver" class="com.csc.cscip.ux.pointin.rest.processor.MediaURLResolver" />

	<!-- Policy ModifierURL -->
	<bean id="policyModifierURLResolver" class="com.csc.cscip.ux.pointin.rest.processor.PolicyModifierURLResolver" />

	<!-- PICS -->
	<bean id="picsDataProcessor" class="com.csc.cscip.ux.pointin.rest.processor.PICSDataProcessor">
		<property name="delegate" ref="xStreamServiceProcessor"></property>
	</bean>

	<bean id="multipartResolver"
		class="org.springframework.web.multipart.commons.CommonsMultipartResolver">

		 <!-- setting maximum upload size -->
		<property name="maxUploadSize" value="400000" />

	</bean>
	<bean class="com.csc.cscip.ux.common.rest.processor.ServiceProcessorFactory">
		<property name="serviceProcessorRegistry">
			<map>
				<entry key="${Application}" value-ref="dataProcessor" />

				<entry key="LRARO" value-ref="editableGridDataProcessor" />
				<entry key="AUDIT" value-ref="editableGridDataProcessor" />
				<entry key="RCV" value-ref="editableGridDataProcessor" />
				<entry key="MNFORMS" value-ref="editableGridDataProcessor"/>
				<entry key="PICS" value-ref="picsDataProcessor" />
				<entry key="ADDCHG" value-ref="editableGridDataProcessor"/>
				<entry key="SUBJ" value-ref="editableGridDataProcessor" />
				<entry key="FORMS" value-ref="editableGridDataProcessor" />
				<entry key="CLT" value-ref="editableGridDataProcessor" />
				<entry key="VEHCOVG" value-ref="editableGridDataProcessor" />
				<entry key="NAVIGATION" value-ref="navigationDataProcessor" />
				<entry key="RETRO" value-ref="editableGridDataProcessor" />
				<entry key="JDBC" value-ref="pointJDBCXMLProcessor" />

				<entry key="SUPPORT" value-ref="supportDataProcessor" />
				<entry key="GRID" value-ref="gridDataProcessor" />
				<entry key="ACTION" value-ref="actionProcessor" />

				<entry key="MEDIA" value-ref="mediaURLResolver" />
				<entry key="POLICYMODIFIER" value-ref="policyModifierURLResolver" />
				
				<entry key="DOJO" value-ref="editableGridDataProcessor" />
				<entry key="Underwriter" value-ref="editableGridDataProcessor" />
                                <entry key="STMT" value-ref="editableGridDataProcessor" />  <!--Key for Statement page -->
			</map>
		</property>
	</bean>

	<context:annotation-config />

	<!-- Controllers -->
	<context:component-scan base-package="com.csc.cscip.ux.pointin.rest" />

	<!-- Camel Context -->

		<!-- define a bean of type StreamCachingStrategy which CamelContext will automatic use -->
		<bean id="streamStrategy" class="org.apache.camel.impl.DefaultStreamCachingStrategy">
		  <property name="spoolDirectory" value="/tmp/cachedir"/>
		  <property name="spoolThreshold" value="9048576"/>
		  <property name="bufferSize" value="9384662"/>
		</bean>
	
