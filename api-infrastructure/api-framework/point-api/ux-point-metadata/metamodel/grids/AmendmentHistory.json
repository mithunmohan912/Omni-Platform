        {
          "startIndex": "5",
          "type": "KEY",
          "endIndex": "8"
        }
      ],
      "polno": [
        {
          "startIndex": "8",
          "type": "KEY",
          "endIndex": "15"
        }
      ],
      "mod": [
        {
          "startIndex": "15",
          "type": "KEY",
          "endIndex": "17"
        }
      ],
      "mco": [
        {
          "startIndex": "3",
          "type": "KEY",
          "endIndex": "5"
        }
      ],
      "loc": [
        {
          "startIndex": "1",
          "type": "KEY",
          "endIndex": "3"
        }
      ]
    }
